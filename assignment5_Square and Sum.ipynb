    "class Point:\n",
    "    def __init__(self,x,y,z):\n",
    "        self.x=x\n",
    "        self.y=y\n",
    "        self.z=z\n",
    "    def sqSum(self):\n",
    "        return(self.x**2+self.y**2+self.z**2)\n",
    "              \n",
    "x=int(input())\n",
    "y=int(input())\n",
    "z=int(input())\n",
    "p1=Point(x,y,z)\n",
    "print(p1.sqSum())"
   ]
