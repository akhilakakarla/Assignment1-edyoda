    "def square_num(n):\n",
    "    return n**2\n",
    "list1 = []\n",
    "n =int(input(\"Enter number of elements in a List: \"))\n",
    "for i in range(0,n):\n",
    "    ele = int(input(\"enter ele: \"))\n",
    "    list1.append(ele)\n",
    "r = map(square_num,list1)\n",
    "print(\"Square the elements of the list:\")\n",
    "print(list(r))"
