    "a=input()\n",
    "even_count=0\n",
    "odd_count=0\n",
    "list1=a.split(\",\")\n",
    "for i in list1:\n",
    "    if(int(i)%2==0):\n",
    "        even_count+=1\n",
    "    else:\n",
    "        odd_count+=1\n",
    "print(\"Number of even numbers : \",even_count)\n",
    "print(\"Number of odd numbers : \",odd_count)\n",
